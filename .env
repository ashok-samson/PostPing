# .env.example

# Set to true to use Azure Managed Identity
USE_MANAGED_IDENTITY=false

# Used when USE_MANAGED_IDENTITY=true
DB_HOST=pgserver.postgres.database.azure.com
DB_NAME=LogStore
DB_USER=pguser@tenant-id

# Used when USE_MANAGED_IDENTITY=false
DATABASE_URL=postgresql://pguser:password@localhost:5432/LogStore
